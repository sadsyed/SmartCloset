<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="InputLinearLayout">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:weightSum">1</item>
        <item name="android:background">#D3D3D3</item>
    </style>

    <style name="InputLabelTextView">
        <item name="android:layout_width">38dp</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:layout_weight">0.23</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="InputEditTextView">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.64</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:background">#fffffbff</item>
    </style>
</resources>
